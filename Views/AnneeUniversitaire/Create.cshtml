@model AnneeUniversitaire

<form id="new-year-form">
    <div class="form-group">
        <label for="" class="control-label">
            Date Début Année Universitaire
        </label>
        <input type="date" class="form-control" asp-for="DateDebut" id="datePicker1">
    </div>

    <div class="form-group">
        <label for="" class="control-label">
            Date Fin Année Universitaire
        </label>
        <input type="date" class="form-control" asp-for="DateFin" id="datePicker2">
    </div>

    <div class="modal-footer btn-full-right">
        <button type="reset" class="btn b-button">Annuler</button>
        <button type="button" class="btn b-button" id="submit">Enregistrer</button>
    </div>
</form>